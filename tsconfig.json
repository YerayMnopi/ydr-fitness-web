{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "strict": true,
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "es2020",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "ydr-ng-common": [
        "node_modules/ydr-ng-common",
      ],
      "ydr-ng-common/auth": [
        "node_modules/ydr-ng-common/auth"
      ],
      "ydr-ng-common/user": [
        "node_modules/ydr-ng-common/user"
      ],
      "ydr-ng-common/notifications": [
        "node_modules/ydr-ng-common/notifications"
      ]
    }
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}
